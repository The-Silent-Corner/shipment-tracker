<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Packages</title>
</head>
<body>
    <h1>Packages</h1>
    <br>
    <% Package.forEach(e => { %>
        <p>
            <span>
                <%- e.id %>
            </span>
            <span>
                <%- e.status %>
            </span>
            <span>
                <%- e.deliveryDate %>
            </span>
        </p>
    <% }) %>
</body>
</html>